# Serializers - Validation messages
msgid "Poet name cannot be empty."
msgstr "نام شاعر نمی‌تواند خالی باشد."

msgid "Category title cannot be empty."
msgstr "عنوان دسته‌بندی نمی‌تواند خالی باشد."

msgid "Verse text cannot be empty."
msgstr "متن مصرع نمی‌تواند خالی باشد."

msgid "Verse order must be a positive number."
msgstr "ترتیب مصرع باید عددی مثبت باشد."

msgid "A verse with this order already exists in this poem."
msgstr "مصرعی با این ترتیب در این شعر وجود دارد."

msgid "Poem title cannot be empty."
msgstr "عنوان شعر نمی‌تواند خالی باشد."

msgid "This verse does not belong to this poem."
msgstr "این مصرع به این شعر تعلق ندارد."

msgid "This verse has already been added to favorites."
msgstr "این مصرع قبلاً به علاقه‌مندی‌ها اضافه شده است."

msgid "Download URL must start with http:// or https://"
msgstr "آدرس دانلود باید با http:// یا https:// شروع شود."

msgid "Time must be a positive number."
msgstr "زمان باید عددی مثبت باشد."

msgid "This audio file does not belong to this poem."
msgstr "این فایل صوتی به این شعر تعلق ندارد."

msgid "A verse with this order does not exist in this poem."
msgstr "مصرع با این ترتیب در این شعر وجود ندارد."

msgid "Font size must be between 8 and 48."
msgstr "اندازه فونت باید بین ۸ تا ۴۸ باشد."

msgid "View mode must be one of: %(modes)s"
msgstr "حالت نمایش باید یکی از این موارد باشد: %(modes)s"

